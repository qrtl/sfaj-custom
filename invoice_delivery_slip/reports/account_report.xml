<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="delivery_slip" model="ir.actions.report">
        <field name="name">Delivery Slip</field>
        <field name="model">account.move</field>
        <field name="report_type">qweb-pdf</field>
        <field
            name="report_name"
        >invoice_delivery_slip.report_invoice_delivery_slip</field>
        <field
            name="report_file"
        >invoice_delivery_slip.report_invoice_delivery_slip</field>
        <field name="print_report_name">(object._get_report_base_filename())</field>
        <field name="binding_model_id" ref="account.model_account_move" />
        <field name="binding_type">report</field>
        <field
            name="groups_id"
            eval="[(4, ref('account.group_account_invoice')), (4, ref('account.group_account_readonly'))]"
        />
    </record>
</odoo>
